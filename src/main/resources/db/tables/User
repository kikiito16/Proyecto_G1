DROP TABLE IF EXISTS User;
CREATE TABLE User (
    id int not null auto_increment,
    username varchar(25) not null unique,
    password varchar(50) not null,
    fullName varchar(100) not null,
    email varchar(100) not null,
    money int not null,
    PRIMARY KEY(id)
);

insert into User (username, password, fullName, email, money) values("juanito123", "1234", "Juan Carlos", "juan@gmail.com", 50);
insert into User (username, password, fullName, email, money) values("sergioaguero", "elkun", "Sergio Aguero", "sergio@gmail.com", 70);
